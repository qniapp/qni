{
  "name": "@qni/docs",
  "private": true,
  "version": "0.0.27",
  "description": "An interactive tutorial for @qni/www.",
  "author": "@yasuhito",
  "homepage": "https://github.com/qniapp/qni#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qniapp/qni.git"
  },
  "scripts": {
    "start": "bundle exec jekyll s",
    "build": "yarn build:css && yarn build:js",
    "build:js": "esbuild src/application.js --bundle --keep-names --minify --sourcemap --outdir=. && esbuild src/serviceworker.js --bundle --keep-names --minify --sourcemap --outdir=.",
    "build:css": "tailwindcss -c tailwind.config.js --postcss -i ./src/application.css -o ./application.css",
    "watch:css": "yarn build:css --watch"
  },
  "bugs": {
    "url": "https://github.com/qniapp/qni/issues"
  },
  "dependencies": {
    "@qni/common": "^0.0.27",
    "@qni/elements": "^0.0.27",
    "@qni/simulator": "^0.0.27"
  },
  "devDependencies": {
    "esbuild": "^0.14.14",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^3.0.17"
  },
  "eslintIgnore": [
    "application.css",
    "application.js",
    "application.js.map",
    "serviceworker.js",
    "serviceworker.js.map"
  ]
}
