{
  "name": "@qni/elements",
  "version": "0.0.21",
  "description": "Quantum circuit components.",
  "author": "@yasuhito",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qniapp/qni.git"
  },
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist build",
    "prelint": "npm run icon",
    "lint": "eslint . --ext .js,.ts && tsc --noEmit",
    "prebuild": "npm run icon && npm run css && npm run lint",
    "build": "node --experimental-json-modules build.js",
    "build:packages": "yarn build",
    "pretest": "npm run build",
    "test": "web-test-runner test/*.js --node-resolve",
    "test:ci": "yarn test",
    "test:watch": "yarn test --watch",
    "css": "tailwindcss -c tailwind.config.cjs --postcss -i ./css/qni.css -o ./dist/qni.css",
    "watch:css": "yarn css --watch",
    "icon": "svg-to-ts --config icon/.svg-to-tsrc",
    "docs": "typedoc --excludeExternals --excludePrivate --excludeProtected"
  },
  "keywords": [
    "quantum",
    "circuit",
    "components",
    "quantum-computing"
  ],
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/qniapp/qni/tree/main/packages/elements#readme",
  "prettier": "@github/prettier-config",
  "dependencies": {
    "@github/catalyst": "^1.1.4",
    "@github/jtml": "^0.4.0",
    "@qni/common": "^0.0.21",
    "interactjs": "^1.10.11",
    "tippy.js": "^6.3.7",
    "xstate": "^4.27.0"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@github/prettier-config": "^0.0.4",
    "@rollup/plugin-commonjs": "^18",
    "@rollup/plugin-replace": "^3.0.1",
    "@tailwindcss/forms": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@web/dev-server-rollup": "^0.3.14",
    "@web/test-runner": "^0.13.25",
    "autoprefixer": "^10.4.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-custom-elements": "^0.0.4",
    "eslint-plugin-github": "^4.3.4",
    "postcss": "^8.3.11",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "~7.2",
    "svg-to-ts": "^7.1.0",
    "tailwindcss": "~3.0",
    "typedoc": "^0.22.8",
    "typescript": "~4.5"
  },
  "eslintIgnore": [
    "dist/",
    "docs/",
    "icon/",
    "tailwind.config.cjs",
    "web-test-runner.config.js"
  ],
  "gitHead": "a735e8013d45ba56fef6f3b6fcec597177f4c779"
}
