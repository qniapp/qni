{
  "name": "@qni/simulator",
  "version": "0.0.86",
  "description": "Quantum simulator for @qni.",
  "author": "@yasuhito",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qniapp/qni.git"
  },
  "main": "index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .js,.ts",
    "typecheck": "tsc --noEmit",
    "build": "node build.js",
    "presize": "npm run build",
    "size": "size-limit",
    "test": "karma start test/karma.config.cjs && npm run size",
    "test:ci": "yarn test",
    "test:watch": "yarn test --auto-watch --no-single-run",
    "docs": "typedoc --excludeExternals --excludePrivate --excludeProtected"
  },
  "keywords": [
    "quantum",
    "circuit",
    "simulator",
    "quantum-computing"
  ],
  "files": [
    "dist"
  ],
  "prettier": "@github/prettier-config",
  "dependencies": {
    "@qni/elements": "^0.0.86",
    "fp-ts": "^2.11.5",
    "karma-esbuild": "^2.2.5",
    "karma-qunit": "^4.1.2",
    "qunit": "^2.19.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/jest": "^29.5.3",
    "@types/karma-chai": "^0.1.3",
    "@types/karma-mocha": "^1.3.1",
    "@types/mocha": "^10.0.1",
    "@types/qunit": "~2.19",
    "chai": "^4.3.4",
    "jest": "^29.6.2",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^2.0.1",
    "mocha": "~10.2",
    "ts-jest": "^29.1.1"
  },
  "eslintIgnore": [
    "dist/",
    "docs/"
  ],
  "size-limit": [
    {
      "limit": "38 kB",
      "path": "dist/index.js"
    }
  ],
  "gitHead": "a735e8013d45ba56fef6f3b6fcec597177f4c779"
}
