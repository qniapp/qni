{
  "name": "@qni/simulator",
  "version": "0.0.84",
  "description": "Quantum simulator for @qni.",
  "author": "@yasuhito",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qniapp/qni.git"
  },
  "main": "index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .js,.ts",
    "typecheck": "tsc --noEmit",
    "build": "node build.js",
    "presize": "npm run build",
    "size": "size-limit",
    "test": "karma start test/karma.config.cjs && npm run size",
    "test:ci": "yarn test",
    "test:watch": "yarn test --auto-watch --no-single-run",
    "docs": "typedoc --excludeExternals --excludePrivate --excludeProtected"
  },
  "keywords": [
    "quantum",
    "circuit",
    "simulator",
    "quantum-computing"
  ],
  "files": [
    "dist"
  ],
  "prettier": "@github/prettier-config",
  "dependencies": {
    "@qni/elements": "^0.0.84",
    "fp-ts": "^2.11.5"
  },
  "devDependencies": {
    "@github/prettier-config": "^0.0.4",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/qunit": "^2.11.2",
    "chai": "^4.3.4",
    "eslint-plugin-github": "^4.3.5",
    "karma": "^6.3.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "mocha": "^9.1.3",
    "size-limit": "^7.0.8",
    "typedoc": "~0.23",
    "typescript": "~4.8"
  },
  "eslintIgnore": [
    "dist/",
    "docs/"
  ],
  "size-limit": [
    {
      "limit": "29 kB",
      "path": "dist/index.js"
    }
  ],
  "gitHead": "a735e8013d45ba56fef6f3b6fcec597177f4c779"
}
