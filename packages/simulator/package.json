{
  "name": "@qni/simulator",
  "version": "0.0.6",
  "description": "Quantum simulator for @qni.",
  "author": "@yasuhito",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yasuhito/qni-simulator.git"
  },
  "main": "index.js",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc",
    "lint": "eslint . --ext .js,.ts && tsc --noEmit",
    "prebuild": "npm run lint && npm run compile",
    "build": "rollup -c",
    "build:packages": "yarn build",
    "test": "karma start test/karma.config.cjs",
    "test:ci": "yarn test",
    "test:watch": "yarn test --auto-watch --no-single-run",
    "docs": "typedoc --excludeExternals --excludePrivate --excludeProtected"
  },
  "keywords": [
    "quantum",
    "circuit",
    "simulator",
    "quantum-computing"
  ],
  "prettier": "@github/prettier-config",
  "dependencies": {
    "@qni/elements": "^0.0.6",
    "fp-ts": "^2.11.5"
  },
  "devDependencies": {
    "@github/prettier-config": "^0.0.4",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/qunit": "^2.11.2",
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "eslint-plugin-github": "^4.3.5",
    "karma": "^6.3.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^9.1.3",
    "rollup": "^2.60.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2"
  },
  "eslintIgnore": [
    "build/",
    "dist/",
    "rollup.config.js"
  ],
  "gitHead": "a735e8013d45ba56fef6f3b6fcec597177f4c779"
}
