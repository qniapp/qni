{
  "name": "@qni/www",
  "version": "0.0.83",
  "description": "Web service for @qni.",
  "author": "@yasuhito",
  "homepage": "https://github.com/qniapp/qni/tree/main/apps/www#readme",
  "license": "MIT",
  "keywords": [
    "quantum",
    "simulator",
    "circuit",
    "components",
    "quantum-computing",
    "quantum-computer-simulator",
    "quantum-algorithms"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yasuhito/qni.git"
  },
  "engines": {
    "yarn": "1.x"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "./bin/rails javascript:clobber",
    "dev": "./bin/dev",
    "lint": "eslint . --ext .js,.ts",
    "typecheck": "tsc --noEmit",
    "build": "yarn build:css && yarn build:js",
    "build:js": "esbuild app/javascript/application.js --bundle --keep-names --sourcemap --minify --outdir=app/assets/builds && esbuild app/assets/javascripts/serviceworker.js --bundle --sourcemap --minify --outdir=app/assets/builds",
    "build:css": "tailwindcss --postcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css",
    "build:npm": "rollup -c && yarn build:css && cp ./app/assets/builds/application.css dist/qni.css",
    "build:docs": "cd docs && JEKYLL_ENV=production bundle exec jekyll build",
    "pretest": "yarn build && yarn build:css",
    "test": "MINITEST_REPORTER=SpecReporter ./bin/rails test:system test",
    "test:ci": "bundle && ./bin/rails test",
    "test:watch": "yarn test --auto-watch --no-single-run",
    "watch": "rollup -wc",
    "prerelease": "yarn build:npm && git --no-pager diff && echo && npm pack --dry-run",
    "release": "npm publish"
  },
  "prettier": "@github/prettier-config",
  "dependencies": {
    "@hotwired/stimulus": "^3.0.1",
    "@qni/common": "^0.0.83",
    "@qni/elements": "^0.0.83",
    "@qni/simulator": "^0.0.83",
    "@rails/ujs": "~7.0",
    "@svgdotjs/svg.filter.js": "^3.0.8",
    "@svgdotjs/svg.js": "^3.1.1",
    "fastdom": "^1.0.10",
    "puppeteer": "~13.1",
    "turbolinks": "^5.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-typescript": "~8.3",
    "autoprefixer": "^10.4.0",
    "esbuild": "^0.14.29",
    "eslint-config-prettier": "~8.3",
    "eslint-import-resolver-typescript": "~2.5",
    "eslint-plugin-github": "~4.3",
    "eslint-plugin-prettier": "~4.0",
    "postcss": "^8.3.11",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "~7.2",
    "rollup": "~2.70",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "~4.2",
    "rollup-plugin-replace": "^2.2.0",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.8"
  },
  "directories": {
    "doc": "doc",
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "eslintIgnore": [
    "app/assets/builds",
    "app/assets/javascripts/serviceworker-companion.js",
    "app/assets/javascripts/serviceworker.js",
    "app/javascript/application.js",
    "app/javascript/controllers/",
    "docs",
    "postcss.config.js",
    "public/assets/",
    "rollup.config.js",
    "tailwind.config.js"
  ],
  "gitHead": "a735e8013d45ba56fef6f3b6fcec597177f4c779"
}
