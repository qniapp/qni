{
  "name": "@qni/www",
  "version": "0.0.1",
  "description": "Web service for @qni.",
  "author": "@yasuhito",
  "homepage": "https://github.com/qniapp/qni/tree/main/apps/www#readme",
  "license": "MIT",
  "keywords": [
    "quantum",
    "simulator",
    "circuit",
    "components",
    "quantum-computing",
    "quantum-computer-simulator",
    "quantum-algorithms"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yasuhito/qni.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "esbuild app/javascript/application.js --bundle --outdir=app/assets/builds && esbuild app/assets/javascripts/serviceworker.js --bundle --outdir=app/assets/builds",
    "build:css": "tailwindcss --postcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css",
    "build:npm": "rollup -c && cp dist/index.js* ./docs/ && yarn build:css && cp ./app/assets/builds/application.css dist/qni.css && cp ./app/assets/builds/application.css docs/qni.css",
    "build:docs": "cd docs && JEKYLL_ENV=production bundle exec jekyll build",
    "build:typedoc": "typedoc --excludeExternals --excludePrivate --excludeProtected --hideGenerator --tsconfig tsconfig.typedoc.json --externalPattern 'node_modules/**/*.ts'",
    "pretest": "yarn build && yarn build:css",
    "test": "karma start && MINITEST_REPORTER=SpecReporter ./bin/rails test:system test",
    "test:ci": "karma start && bundle && ./bin/rails test",
    "test:watch": "yarn test --auto-watch --no-single-run",
    "watch": "rollup -wc",
    "prerelease": "yarn build:npm && git --no-pager diff && echo && npm pack --dry-run",
    "release": "npm publish"
  },
  "prettier": "@github/prettier-config",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "~7.16",
    "@babel/plugin-proposal-throw-expressions": "~7.16",
    "@babel/preset-typescript": "~7.16",
    "@github/catalyst": "^1.1.3",
    "@github/jtml": "^0.4.0",
    "@hotwired/stimulus": "^3.0.1",
    "@interactjs/actions": "^1.10.11",
    "@interactjs/auto-start": "^1.10.11",
    "@interactjs/dev-tools": "^1.10.11",
    "@interactjs/interact": "^1.10.11",
    "@interactjs/modifiers": "^1.10.11",
    "@interactjs/types": "^1.10.11",
    "@qni/common": "^0.0.1",
    "@qni/elements": "^0.0.1",
    "@qni/simulator": "^0.0.1",
    "@rails/ujs": "^6.0.0",
    "@svgdotjs/svg.filter.js": "^3.0.8",
    "@svgdotjs/svg.js": "^3.1.1",
    "@tailwindcss/forms": "^0.3.3",
    "autoprefixer": "^10.4.0",
    "esbuild": "^0.13.12",
    "fraction.js": "^4.1.1",
    "interactjs": "^1.10.11",
    "nouislider": "^15.2.0",
    "postcss": "^8.3.11",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "puppeteer": "~13.0",
    "tailwindcss": "^2.2.19",
    "tippy.js": "~6.3",
    "turbolinks": "^5.2.0",
    "typescript": "~4.4"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-typescript": "~8.3",
    "@types/qunit": "~2.11",
    "@types/typpy": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "~5.3",
    "@typescript-eslint/parser": "~5.3",
    "babel-eslint": "^10.1.0",
    "cz-conventional-changelog": "~3.3",
    "eslint": "~8.1",
    "eslint-config-prettier": "~8.3",
    "eslint-import-resolver-typescript": "~2.5",
    "eslint-plugin-custom-elements": "^0.0",
    "eslint-plugin-github": "~4.3",
    "eslint-plugin-prettier": "~4.0",
    "karma": "~6.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-esbuild": "^2.2.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-qunit": "^4.1.1",
    "lint-staged": "~11.1",
    "prettier": "~2.4",
    "qunit": "~2.16",
    "rollup": "~2.59",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "typedoc": "~0.22"
  },
  "directories": {
    "doc": "doc",
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "dist"
  ]
}
